  "defaults": {
    "build": {
      "output": "canisters/",
      "packtool": ""
    }
  },
  "dfx": "0.5.12-3-g60ea1c9",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "tungsten": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
