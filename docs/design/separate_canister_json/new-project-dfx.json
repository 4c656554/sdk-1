      "main": "src/sample/main.mo",
      "type": "motoko"
    },
    "sample_assets": {
      "dependencies": [
        "sample"
      ],
      "frontend": {
        "entrypoint": "src/sample_assets/public/index.js"
      },
      "source": [
        "src/sample_assets/assets",
        "dist/sample_assets/"
      ],
      "type": "assets"
    }
